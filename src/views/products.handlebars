    <h2>GORRAS DISPONIBLES</h2>
<div class="display">
    {{#each stock}}
    <div class="productContainer">
        <center><img src="{{this.thumbnail}}" alt="not found"></center>
        <div>
            <p>{{this.title}}</p>
            <p>${{this.price}}</p>
        </div>
        <a href="http://localhost:8080/api/cart/{{this._id}}"><button>Agregar al carrito</button></a>
    </div>
    {{/each}}
</div>
<link rel="stylesheet" href="css/product.css">